<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_FFFFFF"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/shape_ed2"
        android:paddingBottom="16dp">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:paddingTop="30dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="       编 号"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_code"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/tv_type_part2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="       类 型"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_type"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="型号名称"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_modelSeriesName"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="产品名称"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_productName"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/tv_productType2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="产品类别"
                        android:textColor="@color/color_FFFFFF" />

                    <TextView
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_productType"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="阶段标记"
                        android:textColor="@color/color_FFFFFF" />

                    <TextView
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_stage"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="创造时间"
                        android:textColor="@color/color_FFFFFF" />

                    <TextView
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_createTime"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:paddingTop="30dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="       名 称"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_name"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/tv_responseUnit2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="责任单位"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_responseUnit"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="型号代号"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_modelCode"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="产品代号"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_productCode"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="       批 次"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_batch"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginTop="24dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="    创建者"
                        android:textColor="@color/color_FFFFFF" />

                    <EditText
                        android:cursorVisible="true"
                        android:textCursorDrawable="@null"
                        android:id="@+id/tv_creator"
                        android:layout_width="300dp"
                        android:layout_height="36dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/shape_edtext"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:textColor="@color/color_FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>


            </LinearLayout>


        </LinearLayout>

    </LinearLayout>


</LinearLayout>