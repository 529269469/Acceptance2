<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:background="@drawable/shape_ed"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp">

        <TextView
            android:id="@+id/tv_project"
            android:textColor="@color/color_FFFFFF"
            android:gravity="center"
            android:text=""
            android:layout_weight="3"
            android:layout_width="0dp"
            android:layout_height="match_parent"/>

        <TextView
            android:id="@+id/tv_name"
            android:textColor="@color/color_FFFFFF"
            android:gravity="center"
            android:text=""
            android:layout_weight="5"
            android:layout_width="0dp"
            android:layout_height="match_parent"/>

        <TextView
            android:id="@+id/tv_typeDisplaye"
            android:textColor="@color/color_FFFFFF"
            android:gravity="center"

            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_height="match_parent"/>
        <TextView
            android:id="@+id/tv_secret"
            android:textColor="@color/color_FFFFFF"
            android:gravity="center"

            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_height="match_parent"/>
        <TextView
            android:id="@+id/tv_stage"
            android:textColor="@color/color_FFFFFF"
            android:gravity="center"

            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_height="match_parent"/>

        <TextView
            android:id="@+id/tv_productCode"
            android:textColor="@color/color_FFFFFF"
            android:gravity="center"
            android:text=""
            android:layout_weight="5"
            android:layout_width="0dp"
            android:layout_height="match_parent"/>

        <TextView
            android:id="@+id/tv_name_file"
            android:textColor="@color/color_FFFFFF"
            android:gravity="center"
            android:text=""
            android:layout_weight="5"
            android:layout_width="0dp"
            android:layout_height="match_parent"/>

    </LinearLayout>

    <com.example.acceptance2.view.MyListView
        android:id="@+id/lvlv_doc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>