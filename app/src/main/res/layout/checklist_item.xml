<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@drawable/shape_ed"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_xuhao"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="序号"
            android:textColor="@color/color_FFFFFF" />


        <TextView
            android:id="@+id/tv_code"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center"
            android:text="编号"
            android:textColor="@color/color_FFFFFF" />

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="名称"
            android:textColor="@color/color_FFFFFF" />


        <TextView
            android:id="@+id/tv_versionInfo"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="版本"
            android:textColor="@color/color_FFFFFF" />


        <TextView
            android:id="@+id/tv_acceptorUnit"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="验收单位（甲方）"
            android:textColor="@color/color_FFFFFF" />


        <TextView
            android:id="@+id/tv_applyCompany"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="乙方验收单位"
            android:textColor="@color/color_FFFFFF" />


        <TextView
            android:id="@+id/tv_modelSeriesName"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="型号名称"
            android:textColor="@color/color_FFFFFF" />


        <TextView
            android:id="@+id/tv_productName"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="产品名称"
            android:textColor="@color/color_FFFFFF" />


        <TextView
            android:id="@+id/tv_productCode"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="产品代号"
            android:textColor="@color/color_FFFFFF" />
        <TextView
            android:id="@+id/tv_productType"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="产品类别"
            android:textColor="@color/color_FFFFFF" />
        <TextView
            android:id="@+id/tv_batch"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="批次"
            android:textColor="@color/color_FFFFFF" />
        <TextView
            android:id="@+id/tv_creator"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="创建者"
            android:textColor="@color/color_FFFFFF" />
        <TextView
            android:id="@+id/tv_createTime"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="创建时间"
            android:textColor="@color/color_FFFFFF" />
        <TextView
            android:id="@+id/tv_versionInfo2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text=""
            android:textColor="@color/color_FFFFFF" />
    </LinearLayout>
</LinearLayout>