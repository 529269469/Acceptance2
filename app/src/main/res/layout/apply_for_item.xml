<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/tv_num"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_productCodeName"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_productName"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_productCode"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_productStatus"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_checkCount"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_isPureCheck"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_isArmyCheck"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_isCompleteChoice"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_isCompleteRoutine"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_isSatisfyRequire"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_description"
        android:textColor="@color/color_FFFFFF"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:textSize="12sp" />

</LinearLayout>